    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "node-mocks-http": "^1.12.1",
    "proxyquire": "^2.1.0",
    "rewiremock": "^3.13.9",
