    "ts-node-dev": "^2.0.0"
  },
  "resolutions": {
    "agenda/cron": "1.8.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:unit"
    }
  },
  "lint-staged": {
    "server/**/*.js": [
      "yarn lint"
    ]
  },
  "imports": {
    "#server/*": {
      "node": "./server/*"
    },
    "#db/*": {
      "node": "./db/*"
    },
    "#root/*": {
      "node": "./*"
    },
    "#test/*": {
      "node": "./test/*"
    },
    "#common/*": {
      "node": "../frontend/common/*"
    }
  },
  "scripts": {
    "db:create": "sequelize db:migrate && sequelize db:seed:all",
    "e2e:db:create": "sequelize --env test db:drop && sequelize --env test db:create && sequelize --env test db:migrate",
    "dev": "ts-node-dev ./server/index.ts --watch server/**/*.d.ts",
    "build": "rm -rf dist && tsc -p tsconfig.production.json && tsc-alias && cp -r server/mails/dist dist/api/server/mails && cp -r assets dist/api && cp .sequelizerc dist/api && cp -r db/data dist/api/db/data",
    "serve": "nodemon server/index.js",
    "start": "node dist/api/server/index.js",
    "test": "yarn mocha -r ts-node/register --require ./test/bootstrap.ts --bail",
    "test:watch": "TS_NODE_TRANSPILE_ONLY=true yarn mocha -r ts-node/register --require ./test/bootstrap.ts --bail --watch --watch-extensions ts --watch-files server",
    "test:unit": "yarn mocha -r ts-node/register --require ./test/bootstrap.ts './**/!(*.int.spec.ts|!(*.spec.ts))' --bail --exit",
    "test:integration": "yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts --bail --exit",
    "test:integration:watch": "TS_NODE_TRANSPILE_ONLY=true yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts --bail --watch --watch-extensions ts --watch-files server './server/**/*.int.spec.ts'",
    "test:integration:all": "yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts './server/**/*.int.spec.ts' --bail --exit",
    "lint": "eslint --max-warnings=0 --ext .ts",
    "lint:all": "eslint --no-error-on-unmatched-pattern --max-warnings=0 --ext .ts server",
    "emails:build": "ts-node server/mails/build.ts"
  }
}