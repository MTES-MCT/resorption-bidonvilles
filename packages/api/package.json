    "sinon-chai": "^3.7.0",
    "sinon-express-mock": "^2.1.0",
    "ts-node-dev": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:unit"
    }
  },
  "lint-staged": {
    "server/**/*.js": [
      "yarn lint"
    ]
  },
  "scripts": {
    "db:create": "sequelize db:migrate && sequelize db:seed:all",
    "dev": "ts-node-dev ./server/index.ts --watch server/**/*.d.ts",
    "build": "tsc -p tsconfig.production.json && tsc-alias && cp -r server/mails/dist dist/server/mails && cp -r assets dist && cp .sequelizerc dist && cp -r db/data dist/db/data",
    "start": "nodemon dist/server/index.js",
    "test": "yarn mocha -r ts-node/register --require ./test/bootstrap.ts --bail",
    "test:watch": "TS_NODE_TRANSPILE_ONLY=true yarn mocha -r ts-node/register --require ./test/bootstrap.ts --bail --watch --watch-extensions ts --watch-files server,test",
    "test:unit": "yarn mocha -r ts-node/register --require ./test/bootstrap.ts './**/!(*.int.spec.ts|!(*.spec.ts))' --bail --exit",
    "test:integration": "yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts --bail --exit",
    "test:integration:watch": "TS_NODE_TRANSPILE_ONLY=true yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts --bail --watch --watch-extensions ts --watch-files server,test './server/**/*.int.spec.ts'",
    "test:integration:all": "yarn mocha -r ts-node/register --require ./test/globalFixtures.ts --require ./test/bootstrap.int.ts './server/**/*.int.spec.ts' --bail --exit",
    "lint": "eslint --max-warnings=0 --ext .ts",
    "lint:all": "eslint --no-error-on-unmatched-pattern --max-warnings=0 --ext .ts server",
    "emails:build": "ts-node server/mails/build.ts"
  }
}